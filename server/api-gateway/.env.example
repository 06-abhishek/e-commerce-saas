# Server Configuration
PORT=<your_server_port>  # Example: 3000

# JWT Configuration (Authentication Tokens)
JWT_SECRET=<your_jwt_secret_key>  # Example: a_long_random_secret_key

# Microservices URLs
AUTH_SERVICE_URL=<your_auth_service_url>  # Example: http://localhost:5001
PRODUCT_SERVICE_URL=<your_product_service_url>  # Example: http://localhost:5002
ORDER_SERVICE_URL=<your_order_service_url>  # Example: http://localhost:5003
CART_SERVICE_URL=<your_cart_service_url>  # Example: http://localhost:5004
ADDRESS_SERVICE_URL=<your_address_service_url>  # Example: http://localhost:5005
PAYMENT_SERVICE_URL=<your_payment_service_url>  # Example: http://localhost:5006
REVIEW_SERVICE_URL=<your_review_service_url>  # Example: http://localhost:5007
SEARCH_SERVICE_URL=<your_search_service_url>  # Example: http://localhost:5008
FEATURE_SERVICE_URL=<your_feature_service_url>  # Example: http://localhost:5009